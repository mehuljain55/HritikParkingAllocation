-- Table for ParkingModel
CREATE TABLE Parking (
    parkingId INT ,
    userId INT ,
    parkingHistoryId INT,
    userName VARCHAR(100) ,
    status VARCHAR(50) ,
    parkingType VARCHAR(50)
);

-- Table for ParkingHistory
CREATE TABLE ParkingHistory (
    sno INT ,
    parkingSlot INT ,
    employeeId INT,
    employeeName VARCHAR(100) ,
    date DATE ,
    startTime TIME,
    endTime TIME
);

CREATE TABLE users (
    userId INT PRIMARY KEY  ,
    name VARCHAR(100) ,
    role VARCHAR(50) ,
    password VARCHAR(255)
);
